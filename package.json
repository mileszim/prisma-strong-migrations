  },
  "bugs": {
    "url": "https://github.com/mileszim/prisma-strong-migrations/issues"
  },
  "homepage": "https://github.com/mileszim/prisma-strong-migrations",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "cosmiconfig": "^8.0.0",
    "glob": "^8.0.3",
    "sql-parser-cst": "^0.24.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
