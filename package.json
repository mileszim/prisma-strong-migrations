  "description": "A linter for Prisma migrations to ensure safe SQL deployments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "prisma-strong-migrations": "dist/cli.js"
  },
  "scripts": {
